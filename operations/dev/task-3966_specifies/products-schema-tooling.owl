    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingCostPerPiece -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingCostPerPiece">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>COST/PC</rdfs:label>
        <rdfs:label xml:lang="zh">每件成本</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (?val as ?metalStampingCostPerPiece)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:StampingProcess .
            ?subject pcs:metalStampingQtyPerHr ?qph .
            ?subject pcs:metalStampingRatePerHr ?rph .
            BIND (xsd:decimal(?rph / ?qph) AS ?val) .
          }
        </ps:sparql>
        <ps:sequenceID>9</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
        <ps:group rdf:resource="&products-schema;StampingProcess"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


