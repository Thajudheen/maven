    <!-- http://www.inmindcomputing.com/application/products/products-schema-tooling.owl#metalStampingMachineType -->

    <owl:DatatypeProperty rdf:about="&pcst;metalStampingMachineType">
        <rdfs:label>Machine Type</rdfs:label>
        <rdfs:label xml:lang="zh">机器类型</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?metalStampingMachineType)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:StampingProcess .
            ?subject pcs:hasMachineCode ?code .
            ?code pcs:specifiesMachineType ?type .
            ?type pcs-kb:machineTypeValue ?val.
}
</ps:sparql>
        <ps:sequenceID>35</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
