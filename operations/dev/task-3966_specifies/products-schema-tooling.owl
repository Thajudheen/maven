    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#hasMachineCode -->

    <owl:ObjectProperty rdf:about="&products-schema;hasMachineCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>M/C</rdfs:label>
        <rdfs:label xml:lang="zh">机械码</rdfs:label>
        <ps:mandatory>true</ps:mandatory>
        <ps:enumerable>true</ps:enumerable>
        <ps:sequenceID>3</ps:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
        <ps:group rdf:resource="&products-schema;StampingProcess"/>
        <rdfs:subPropertyOf rdf:resource="&ps;has"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#Machine"/>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
                                <owl:inverseOf rdf:resource="&products-schema;specifiesMachine"/>
                            </rdf:Description>
                        </owl:onProperty>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/application-schema.owl#Company"/>
                                    <owl:Restriction>
                                        <owl:onProperty>
                                            <rdf:Description>
                                                <owl:inverseOf rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl#hasCompany"/>
                                            </rdf:Description>
                                        </owl:onProperty>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/application-schema.owl#Quote"/>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl#includesSalesItem"/>
                                                        <owl:someValuesFrom rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl#SalesItem"/>
                                                    </owl:Restriction>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#hasRate -->

    <owl:ObjectProperty rdf:about="&products-schema;hasRate">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>RATE</rdfs:label>
        <rdfs:label xml:lang="zh">率</rdfs:label>
        <ps:enumerable>true</ps:enumerable>
        <ps:mandatory>true</ps:mandatory>
        <ps:sequenceID>4</ps:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
        <ps:group rdf:resource="&products-schema;StampingProcess"/>
        <rdfs:subPropertyOf rdf:resource="&ps;has"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#Rate"/>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
                                <owl:inverseOf rdf:resource="&products-schema;specifiesRate"/>
                            </rdf:Description>
                        </owl:onProperty>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/application-schema.owl#Company"/>
                                    <owl:Restriction>
                                        <owl:onProperty>
                                            <rdf:Description>
                                                <owl:inverseOf rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl#hasCompany"/>
                                            </rdf:Description>
                                        </owl:onProperty>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/application-schema.owl#Quote"/>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl#includesSalesItem"/>
                                                        <owl:someValuesFrom rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl#SalesItem"/>
                                                    </owl:Restriction>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


