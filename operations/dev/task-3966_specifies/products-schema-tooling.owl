        <rdfs:label xml:lang="zh">过多少件</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;int">0</ps:defaultValue>
        <ps:sequenceID>21</ps:sequenceID>
        <ps:mandatory>true</ps:mandatory>
        <rdfs:subPropertyOf rdf:resource="&pcst;toolingData"/>
        <rdfs:domain rdf:resource="&products-schema;MetalStamping-KB"/>
        <ps:group rdf:resource="&products-schema;Processes"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingQtyPerHr -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingQtyPerHr">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>QTY/HR</rdfs:label>
        <rdfs:label xml:lang="zh">每小时数量</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (?val as ?metalStampingQtyPerHr)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:StampingProcess .
            ?subject pcs:metalStampingSpm ?spm .
            ?subject pcs:metalStampingEff ?eff .
            BIND (xsd:decimal(?spm * ?eff / 100 * 60) AS ?val ) .
          }
        </ps:sparql>
        <ps:sequenceID>7</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
        <ps:group rdf:resource="&products-schema;StampingProcess"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingQtyPerRun -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingQtyPerRun">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>QTY/RUN</rdfs:label>
        <rdfs:label xml:lang="zh">每次运行数量</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string"> select ?subject ?metalStampingQtyPerRun
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem1 .
            ?salesItem1 as:isProduct pci:StampingProcess .
            ?salesItem1 as:salesItemIncludedBy ?salesItem2 .
            ?salesItem2 as:isProduct pci:MetalStamping .
            ?salesItem2 as:includesConfigItem ?config2 .
            ?config2 pcs:metalStampingQtyMth ?metalStampingQtyPerRun .
          }
        </ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID>11</ps:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
        <ps:group rdf:resource="&products-schema;StampingProcess"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
