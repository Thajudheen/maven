    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingRatePerHr -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingRatePerHr">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>RATE/HR</rdfs:label>
        <rdfs:label xml:lang="zh">每小时率</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">
        SELECT ?subject ?metalStampingRatePerHr
    WHERE {
      ?subject as:configItemIncludedBy ?salesItem .
      ?salesItem as:isProduct pci:StampingProcess .
      ?subject pcs:hasMachineCode ?code .
      ?subject pcs:hasRate ?rate .
      ?code pcs:specifiesMachineCostPerHour ?cost .
      ?rate pcs:specifiesMachineCostPerHour ?cost .
      ?cost pcs-kb:machineCostPerHourValue ?costValue .
      ?salesItem as:salesItemIncludedBy+ ?quote .
      ?quote rdf:type as:Quote .
    OPTIONAL{
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:hasTargetCurrency ai:USD.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      }
    BIND ( IF ( EXISTS {
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:hasTargetCurrency ai:USD.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      },
      xsd:decimal(?costValue * (1/?rateValue)),
      ?costValue) AS ?metalStampingRatePerHr )
  }

</ps:sparql>
