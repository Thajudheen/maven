    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingTMaint -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingTMaint">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>T/MAINT</rdfs:label>
        <rdfs:label xml:lang="zh">模具维修</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject ?metalStampingTMaint
    WHERE {
      ?subject as:configItemIncludedBy ?salesItem .
      ?salesItem as:isProduct pci:StampingProcess .
      ?salesItem as:salesItemIncludedBy ?majorSi .
      ?majorSi as:isProduct pci:MetalStamping .
      ?majorSi as:includesConfigItem ?c .
      ?subject pcs:hasMachineCode ?code .
      ?c pcsm:hasThickness ?thick .
        OPTIONAL{
        ?c pcs:hasMaterialSpecification ?spec .
        ?spec pcs:specifiesToolingMaintenance ?tm1 .
            }
        OPTIONAL{
        ?c pcsm:hasToolingMaintenanceOther ?tm2 .
            }
        BIND(IF(NOT EXISTS{
        ?c pcs:hasMaterialSpecification ?spec .
        ?spec pcs:specifiesToolingMaintenance ?tm1 .
      }, ?tm2, ?tm1) AS ?tm)
      ?code pcs:specifiesToolingMaintenanceCost ?cost .
      ?thick pcs:specifiesToolingMaintenanceCost ?cost .
      ?tm pcs:specifiesToolingMaintenanceCost ?cost .
      ?cost pcs-kb:toolingMaintenanceCostValue ?costValue .
      ?salesItem as:salesItemIncludedBy+ ?quote .
      ?quote rdf:type as:Quote .
    OPTIONAL{
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:hasTargetCurrency ai:USD.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      }
    BIND ( IF ( EXISTS {
      ?quote as:includeExchangeRateSalesDocument ?rateDoc .
      ?quote as:hasBaseCurrency ?targetCurrency .
      ?rateDoc as:hasTargetCurrency ai:USD.
      ?rateDoc as:hasBaseCurrency ?targetCurrency .
      ?rateDoc as:exchangeRate ?rateValue .
      },
      xsd:decimal(?costValue * (1/ ?rateValue)  / 1000),
      xsd:decimal(?costValue / 1000)) AS ?metalStampingTMaint )
  }
</ps:sparql>
