    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingOperation -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingOperation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>Operation Type</rdfs:label>
        <rdfs:label xml:lang="zh">操作类型</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?metalStampingOperation)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:StampingProcess .
            ?subject pcs:hasMachineCode ?code .
            ?code pcs:specifiesOperationType ?operation .
            ?operation pcs-kb:operationValue ?val.
}
</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID>1</ps:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB"/>
        <ps:group rdf:resource="&products-schema;StampingProcess"/>
