				<rs:reportDescription rdf:datatype="&xsd;string">Download excel to find out account contact detail&apos;s information.</rs:reportDescription>
				<ps:sparql rdf:datatype="&xsd;string">PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
		PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
		PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
		PREFIX rdfs: &lt;http://www.w3.org/2000/01/rdf-schema#&gt;
		PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;
		PREFIX ps: &lt;http://www.inmindcloud.com/application/schema.owl#&gt;
		PREFIX as: &lt;http://www.inmindcloud.com/application/schema.owl#&gt;
		PREFIX pcs: &lt;http://www.inmindcloud.com/application/products/products-schema.owl#&gt;
		PREFIX pci: &lt;http://www.inmindcloud.com/application/products/products-implementation.owl#&gt;
		PREFIX as-ext: &lt;http://www.inmindcloud.com/application/application-schema-ext.owl#&gt;
		PREFIX rs: &lt;http://www.inmindcloud.com/application/report-schema.owl#&gt;
		PREFIX afn: &lt;http://jena.hpl.hp.com/ARQ/function#&gt;
CONSTRUCT {
			?uuid a rs:AccountAnalyticData;
			a ps:Data;
			rs:accountName ?accountName;
			rs:status ?accountStatusStr;
			rs:accountProspect ?accountProspect;
			rs:industryName ?industryName;
			rs:firstName ?contactFirstName;
			rs:lastName ?contactLastName;
			rs:title ?contactTitleStr;
			rs:gender ?contactGenderStr;
			rs:mobile ?contactMobile;
			rs:phone ?contactPhone;
			rs:fax ?contactFax;
			rs:email ?contactEmail;
			rs:position ?contactPosition;
			rs:street ?street;
			rs:unitNo ?unitNo;
			rs:zip ?zip;
			rs:city ?city;
			rs:country ?countryName;
} WHERE
{
	{
		SELECT ?accountName ?accountStatusStr ?accountProspect ?industryName
						?contact ?contactFirstName ?contactLastName ?contactTitleStr ?contactGenderStr ?contactMobile ?contactPhone ?contactFax ?contactEmail ?contactPosition
						?street ?unitNo ?zip ?city ?countryName
		WHERE {
			?acc rdf:type as:Account .
			?acc ps:objectName ?accountName .
			?acc as:hasAccountStatus ?accountStatus
			BIND(str(?accountStatus) as ?accountStatusStr) .
			OPTIONAL { ?acc as:accountProspect ?accountProspect . }
			?acc as:hasIndustry ?industry .
			?industry ps:objectName ?industryName

			OPTIONAL {
			?acc as:includesPerson ?contact .
			?contact as:personFirstName ?contactFirstName .
			?contact as:personLastName ?contactLastName .
			OPTIONAL {
			?contact as:hasTitle ?contactTitle .
			BIND(str(?contactTitle) as ?contactTitleStr) .
			}
			OPTIONAL {
			?contact as:hasGender ?contactGender .
			BIND(str(?contactGender) as ?contactGenderStr) .
			}
			OPTIONAL { ?contact as:personMobile ?contactMobile . }
			OPTIONAL {	?contact as:personPhone ?contactPhone . }
			OPTIONAL {	?contact as:personFax ?contactFax . }
			OPTIONAL {	?contact as:personEmail ?contactEmail . }
			OPTIONAL {	?contact as:personPosition ?contactPosition . }

			OPTIONAL { ?address as:addressIncludedBy ?contact . }
			OPTIONAL { ?address as:addressStreet ?street . }
			OPTIONAL { ?address as:addressUnitNo ?unitNo . }
			OPTIONAL { ?address as:addressZip ?zip . }
			OPTIONAL { ?address as:addressCity ?city . }
			OPTIONAL { ?address as:hasCountry ?country .
								?country ps:objectName ?countryName.
							}
			}
		} ORDER BY ?accountName ?industryName ?accountStatusStr
	}
	BIND(afn:uuid() as ?uuid).
}</ps:sparql>
