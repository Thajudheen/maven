    <!-- http://www.inmindcomputing.com/application/products/products-schema-process.owl#processMachineRatePerHr -->

    <owl:DatatypeProperty rdf:about="http://www.inmindcomputing.com/application/products/products-schema-process.owl#processMachineRatePerHr">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Work Station Rate/HR</rdfs:label>
        <rdfs:label xml:lang="zh">工作设备成本/每小时</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject ?processMachineRatePerHr
WHERE {
   ?subject  pcsp:hasStation ?ws .
   ?ws  pcs-kb:definesMSUCostPerHr ?costPerHr .
   ?subject  pcsp:hasMSURate ?rate .
   ?rate  pcs-kb:definesMSUCostPerHr ?costPerHr .
   ?costPerHr pcs-kb:processMSUCostPerHr ?rawValue .
   ?subject as:configItemIncludedBy ?salesItem .
   OPTIONAL {?salesItem as:isProduct pci:StampingProcess .}
   OPTIONAL {?salesItem as:isProduct pci:SecondaryProcess .}
   OPTIONAL {?salesItem as:isProduct pci:SecondaryFinishingProcess .}
   ?salesItem as:salesItemIncludedBy+ ?quote .
   ?quote rdf:type as:Quote .
    OPTIONAL{
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        as:SALES_SYSTEM_INSTANCE as:hasDefaultCurrency ?defaultCurrency.
      ?rateDoc as:hasTargetCurrency ?defaultCurrency.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      }
    BIND ( IF ( EXISTS {
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        as:SALES_SYSTEM_INSTANCE as:hasDefaultCurrency ?defaultCurrency.
        ?rateDoc as:hasTargetCurrency ?defaultCurrency.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      },
      xsd:decimal(?rawValue * (1/?rateValue)),
      ?rawValue) AS ?processMachineRatePerHr)
}</ps:sparql>
        <ps:sequenceID>10</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#Process-KB"/>
        <ps:group rdf:resource="&products-schema;ProcessGroup"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-tooling.owl#metalStampingLabourCostPerHour -->

    <owl:DatatypeProperty rdf:about="&pcst;metalStampingLabourCostPerHour">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <ps:sparql>SELECT ?subject ?metalStampingLabourCostPerHour
WHERE{
   ?subject pcsp:hasWorkStation ?workStation .
   ?workStation pcs-kb:workStationLabourHours ?hours .
   ?workStation pcs-kb:workStationLabourRatioOperation ?operationRatio .
   ?workStation pcs-kb:workStationLabourRatioPackaging ?packagingRatio .
   ?workStation pcs-kb:workStationLabourRatioQuality ?qualityRatio .
   ?factory pcs-kb:includesWorkStation ?workStation .
   ?factory pcs-kb:includesLabourCost ?labourCost .
   ?labourCost pcs-kb:labourCostOperation ?operationCost .
   ?labourCost pcs-kb:labourCostPackaging ?packagingCost .
   ?labourCost pcs-kb:labourCostQuality ?qualityCost .
   BIND (xsd:decimal(?hours *
         (?operationCost * ?operationRatio
            + ?packagingCost * ?packagingRatio
            + ?qualityCost * ?qualityRatio)
      ) AS ?rawValue)
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:isProduct pci:StampingProcess .
   ?salesItem as:salesItemIncludedBy+ ?quote .
   ?quote rdf:type as:Quote .
    OPTIONAL{
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        as:SALES_SYSTEM_INSTANCE as:hasDefaultCurrency ?defaultCurrency.
      ?rateDoc as:hasTargetCurrency ?defaultCurrency.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      }
    BIND ( IF ( EXISTS {
        ?quote as:includeExchangeRateSalesDocument ?rateDoc .
        ?quote as:hasBaseCurrency ?targetCurrency .
        as:SALES_SYSTEM_INSTANCE as:hasDefaultCurrency ?defaultCurrency.
      ?rateDoc as:hasTargetCurrency ?defaultCurrency.
        ?rateDoc as:hasBaseCurrency ?targetCurrency .
        ?rateDoc as:exchangeRate ?rateValue .
      },
      xsd:decimal(?rawValue * (1/?rateValue)),
      ?rawValue) AS ?metalStampingLabourCostPerHour)
}</ps:sparql>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#StampingProcess-KB2"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



