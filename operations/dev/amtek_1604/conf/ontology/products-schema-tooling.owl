    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#metalStampingToolCostWithTransportUSD -->

    <owl:DatatypeProperty rdf:about="&products-schema;metalStampingToolCostWithTransportUSD">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>Total Tooling Cost (With Transport)</rdfs:label>
        <rdfs:label xml:lang="zh">总模具成本（含运输）</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject ?metalStampingToolCostWithTransportUSD
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:isProduct pci:MetalStamping .
   ?subject pcs:metalStampingToolMarkup ?markUp .
   ?subject pcs:metalStampingTtlTool ?ttc .
   ?subject pcst:metalStampingTransportCost ?trc .
   ?subject pcs:metalStampingToAmortize ?ta .
   BIND (xsd:decimal(?ttc*(1+(?markUp/100)) + ?trc - ?ta) AS ?rawValue)
   ?salesItem as:salesItemIncludedBy+ ?quote .
   ?quote rdf:type as:Quote .
   OPTIONAL{
