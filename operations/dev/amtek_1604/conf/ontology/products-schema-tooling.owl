    <!-- http://www.inmindcomputing.com/application/products/products-schema-tooling.owl#metalStampingOutputPerHour -->

    <owl:DatatypeProperty rdf:about="&pcst;metalStampingOutputPerHour">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>Output per Hour</rdfs:label>
        <rdfs:label xml:lang="zh">每小时产出</rdfs:label>
        <application-schema:precision rdf:datatype="&xsd;int">0</application-schema:precision>
        <ps:sparql>SELECT ?subject ?metalStampingOutputPerHour
WHERE {
   OPTIONAL {
    ?subject pcs:hasMachineCode ?code1 .
     ?code1 pcs-kb:definesOutputPerHour ?output .
     ?output pcs-kb:outputPerHourValue ?value1.
   }
   OPTIONAL {
      ?subject pcsp:hasWorkStation ?code2 .
      ?code2 pcs-kb:stampingStationOutputPerHour ?value2 .
   }
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:isProduct pci:StampingProcess .
   BIND(IF(!BOUND(?code1),
         IF(!BOUND(?code2),
           xsd:decimal(0), ?value2), ?value1) AS ?metalStampingOutputPerHour) .
}</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
