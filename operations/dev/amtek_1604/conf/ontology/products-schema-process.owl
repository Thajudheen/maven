                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&application-schema;Plant"/>
                                    <owl:Restriction>
                                        <owl:onProperty>
                                            <rdf:Description>
                                                <owl:inverseOf rdf:resource="http://www.inmindcomputing.com/application/application-schema-ext.owl#zhasPlant"/>
                                            </rdf:Description>
