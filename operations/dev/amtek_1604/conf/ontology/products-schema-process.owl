        <platform-schema:mandatory>true</platform-schema:mandatory>
        <application-schema:costingRelevant>true</application-schema:costingRelevant>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#Secondary-KB"/>
        <rdfs:subPropertyOf rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-process.owl#processData"/>
