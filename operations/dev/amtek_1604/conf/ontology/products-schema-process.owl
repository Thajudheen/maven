        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Labour Cost Per Hour</rdfs:label>
        <platform-schema:sequenceID>1</platform-schema:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#ProcessStation"/>
        <rdfs:subPropertyOf rdf:resource="&platform-schema;assertion"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.inmindcomputing.com/application/products/products-schema-process.owl#processLabourCostPerPiece -->

    <owl:DatatypeProperty rdf:about="http://www.inmindcomputing.com/application/products/products-schema-process.owl#processLabourCostPerPiece">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Labour Cost / Pc</rdfs:label>
        <rdfs:label xml:lang="zh">劳动力成本 / 每件</rdfs:label>
        <platform-schema:sparql rdf:datatype="&xsd;string">SELECT ?subject ?processLabourCostPerPiece
WHERE {
   ?subject pcsp:hasStation ?ws.
   ?ws      pcsp:processLabourCostPerHour ?lcph.
   ?subject pcsp:processLabourHoursPerRun ?lhpr.
   ?subect pcsp:processQtyPerRun ?qty.
   BIND (xsd:decimal(?lcph * ?lhpr / ?qty) AS ?processLabourCostPerPiece)
}</platform-schema:sparql>
        <platform-schema:sequenceID>11</platform-schema:sequenceID>
        <platform-schema:description>Labour Hours per Run * Labour Cost per Hour (from Machine Master Data ) / Qty per Run</platform-schema:description>
        <platform-schema:readOnly>true</platform-schema:readOnly>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#Secondary-KB"/>
        <platform-schema:group rdf:resource="http://www.inmindcomputing.com/application/products/products-schema.owl#ProcessGroup"/>
        <rdfs:subPropertyOf rdf:resource="&platform-schema;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.inmindcomputing.com/application/products/products-schema-process.owl#processLabourHoursPerRun -->

    <owl:DatatypeProperty rdf:about="http://www.inmindcomputing.com/application/products/products-schema-process.owl#processLabourHoursPerRun">
