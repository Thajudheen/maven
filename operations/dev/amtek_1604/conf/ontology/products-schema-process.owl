        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#ProcessStation"/>
                    <owl:Restriction>
                        <owl:onProperty>
                            <rdf:Description>
