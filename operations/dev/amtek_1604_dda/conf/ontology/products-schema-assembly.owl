        <rdfs:label rdf:datatype="&xsd;string">Packaging</rdfs:label>
        <rdfs:label xml:lang="zh">包装</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyPackagingSummary)
WHERE {
OPTIONAL
{
   ?subject psa:assemblyPackaging ?val1 .
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:isProduct pci:Assembly .
}
   OPTIONAL
   {
