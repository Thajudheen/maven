    <!-- http://www.inmindcomputing.com/application/products/products-schema-assembly.owl#assemblyCOPA -->

    <owl:DatatypeProperty rdf:about="&psa;assemblyCOPA">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>COPA</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyCOPA)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .

            ?subject psa:assemblyVAT ?vat .
            ?subject psa:assemblySellingPrice ?price .
            ?subject psa:assemblyRawMaterialCost ?material .
            ?subject psa:assemblySubcon ?subcon .
            ?subject psa:assemblyPackagingSummary ?packaging .
            ?subject psa:assemblyPurchaseItem ?purchase .
      ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?price - (?vat + ?material + ?subcon + ?packaging + ?purchase)) AS ?val)
        }
</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <application-schema:costingRelevantSequence>45</application-schema:costingRelevantSequence>
        <application-schema:costingRelevant>true</application-schema:costingRelevant>
        <ps:sequenceID>10</ps:sequenceID>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;Summary"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-assembly.owl#assemblyCOPAUSD -->

    <owl:DatatypeProperty rdf:about="&psa;assemblyCOPAUSD">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>COPA</rdfs:label>
        <ps:sequenceID>10</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <ps:sparql>SELECT ?subject ?assemblyCOPAUSD
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .
   ?subject psa:assemblyVAT ?vat .
   ?subject psa:assemblySellingPrice ?price .
   ?subject psa:assemblyRawMaterialCost ?material .
   ?subject psa:assemblySubcon ?subcon .
   ?subject psa:assemblyPackagingSummary ?packaging .
   ?subject psa:assemblyPurchaseItem ?purchase .
   ?salesItem as:isProduct pci:Assembly .
   BIND (xsd:decimal(?price - (?vat + ?material + ?subcon + ?packaging + ?purchase)) AS ?rawValue)
   ?salesItem as:salesItemIncludedBy+ ?quote .
   ?quote rdf:type as:Quote .
   OPTIONAL{
      ?quote as:includeExchangeRateSalesDocument ?rateDoc .
      ?quote as:hasBaseCurrency ?baseCurrency .
      as:SALES_SYSTEM_INSTANCE as:hasDefaultCurrency ?defaultCurrency.
      ?rateDoc as:hasTargetCurrency ?defaultCurrency.
      ?rateDoc as:hasBaseCurrency ?targetCurrency .
      ?rateDoc as:exchangeRate ?rateValue .
   }
   BIND ( IF ( EXISTS {
      ?quote as:includeExchangeRateSalesDocument ?rateDoc .
      ?quote as:hasBaseCurrency ?baseCurrency .
      as:SALES_SYSTEM_INSTANCE as:hasDefaultCurrency ?defaultCurrency.
      ?rateDoc as:hasTargetCurrency ?defaultCurrency.
      ?rateDoc as:hasBaseCurrency ?targetCurrency .
      ?rateDoc as:exchangeRate ?rateValue
         }, xsd:decimal(?rawValue * ?rateValue),
         ?rawValue) AS ?assemblyCOPAUSD)
}</ps:sparql>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;Group_SummaryUSD"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-assembly.owl#assemblyCurrentPrice -->

    <owl:DatatypeProperty rdf:about="&psa;assemblyCurrentPrice">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val AS ?assemblyCurrentPrice)
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .

   ?subject pcs:assemblySubTotalE  ?part1 .
   ?subject pcs:assemblySubTotalF  ?part2 .
   ?subject psa:assemblyTransportFinalCost  ?part3 .
    ?salesItem as:isProduct pci:Assembly .
   BIND (xsd:decimal(?part1 + ?part2 +?part3) AS ?val)
}</ps:sparql>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-assembly.owl#assemblyFreight -->

    <owl:DatatypeProperty rdf:about="&psa;assemblyFreight">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>Freight</rdfs:label>
        <rdfs:label xml:lang="zh">运费</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyFreight)
          WHERE {
          ?subject as:configItemIncludedBy ?salesItem .

          ?subject psa:assemblyTransportFreightCost ?freight .
          ?subject psa:assemblyTransportHubCost ?hub .
       ?salesItem as:isProduct pci:Assembly .
          BIND (xsd:decimal(?freight + ?hub) AS ?val)
