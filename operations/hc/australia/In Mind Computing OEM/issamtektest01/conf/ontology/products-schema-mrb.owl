    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#mrbRecoverAmount -->

    <owl:DatatypeProperty rdf:about="&pcs;mrbRecoverAmount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Recover Amount (MRB)</rdfs:label>
        <rdfs:label xml:lang="zh">恢复数目</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?mrbRecoverAmount)
WHERE {
   OPTIONAL{
      OPTIONAL{?subject pcs:mrbNonMFGToolAmortization ?ta1 .}
      OPTIONAL{?subject pcs:mrbNonMFGToolingCost ?tc1 .}
      OPTIONAL{?subject pcs:mrbNonMFGFixtureCost ?fc1.}
      OPTIONAL{?subject pcs:mrbNonMFGQAFixtureCost ?qa1.}
   }

   OPTIONAL{
      ?subject pcs:metalStampingToolAmortizationMRB ?ta2 .
      ?subject pcs:metalStampingToolingCostMRB ?tc2 .
   }
   BIND(IF(BOUND(?ta1),?ta1,xsd:decimal(0)) as ?ta)
   BIND(IF(BOUND(?tc1),?tc1,xsd:decimal(0)) as ?tc)
   BIND(IF(BOUND(?fc1),?fc1,xsd:decimal(0)) as ?fc)
   BIND(IF(BOUND(?qa1),?qa1,xsd:decimal(0)) as ?qa)

   BIND (IF (EXISTS {
            ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:MetalStamping.
         }, (xsd:decimal(?tc2 - ?ta2)), (xsd:decimal((?tc + ?fc + ?qa) - ?ta)))  AS ?val)
}</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <ps:group rdf:resource="&pcs;MRBTooling"/>
        <rdfs:domain rdf:resource="&pcs;Tooling-MRB-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#mrbRecoverQuoted -->

    <owl:DatatypeProperty rdf:about="&pcs;mrbRecoverQuoted">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Recover / Quoted (MRB)</rdfs:label>
        <rdfs:label xml:lang="zh">恢复/报价</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?mrbRecoverQuoted)
          WHERE {
            ?subject pcs:mrbRecoverAmount ?ra .
            ?subject pcs:mrbToolingMarkup ?tm .
            BIND (xsd:decimal(?ra * (1 + (?tm / 100))) AS ?val)
        }</ps:sparql>
