    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#mrbNonMFGActCostPerSet -->

    <owl:DatatypeProperty rdf:about="&pcs;mrbNonMFGActCostPerSet">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Act. Cost / Set</rdfs:label>
        <rdfs:label xml:lang="zh">每套实际成本</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?mrbNonMFGActCostPerSet)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:includesItemHeaderPriceItem ?headerPriceItem .
            ?headerPriceItem as:itemHeaderQuantity ?cq .
            ?subject pcs:mrbNonMFGactlCostPerPiece ?acpp .
            BIND (xsd:decimal(?acpp * ?cq) AS ?val)
        }</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">6</ps:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#CostItem-KB"/>
