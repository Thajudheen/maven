        <ps:group rdf:resource="&pcs;Sales"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#mrbNonMFGCostPerPiece -->

    <owl:DatatypeProperty rdf:about="&pcs;mrbNonMFGCostPerPiece">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Cost/pc</rdfs:label>
        <rdfs:label xml:lang="zh">每件成本</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?mrbNonMFGCostPerPiece)
WHERE {
   ?subject pcs:mrbNonMFGMarkUp ?mu .
   OPTIONAL{?subject pcs:mrbNonMFGactlCostPerPiece ?acp . }

   #powder coating and masking under InhouseFinishing
   OPTIONAL{
      ?subject pcs:inhouseFinishingTotalPurchasedCost ?picost.
      ?subject as:configItemIncludedBy ?salesItem.
      ?salesItem as:isProduct pci:InhouseFinishing.
   }

   #recycling calculation
   OPTIONAL { ?subject pcs:purchaseItemRecyclingCostPerPiece ?recycling. }

   #DieCasting LostCost
   OPTIONAL
   {
   ?subject pcs:isFinishingAfterDieCasting ?processAfterDieCasting.
   ?subject pcs:finishingLossCost ?loss.
   }

   #BIND(xsd:decimal((?picost) * (1 + (?mu / 100))) as ?pival)
   BIND(xsd:decimal(?acp * (1 + (?mu / 100))) as ?acpval)
   BIND(IF(?loss != &quot;-&quot;, ?loss,xsd:decimal(0)) as ?l)

   BIND(
      IF(BOUND(?picost) &amp;&amp; ?picost != 0, ?picost + ?acpval,
         IF(BOUND(?recycling) &amp;&amp; ?recycling != 0,?recycling,
            IF(BOUND(?acp) &amp;&amp; ?acp != &quot;-&quot; &amp;&amp; (!BOUND(?processAfterDieCasting) || !?processAfterDieCasting) ,?acpval,
                  IF(?processAfterDieCasting = true, (?acp * (1+?mu/100) + ?l ),
                  xsd:decimal(0))))) as ?val)

}
</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">11</ps:sequenceID>
        <rdfs:domain rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#CostItem-KB"/>
        <ps:group rdf:resource="&pcs;PurchasePart"/>
        <ps:group rdf:resource="&pcs;Sales"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



