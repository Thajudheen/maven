<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ps "http://www.inmindcomputing.com/platform/platform-schema.owl#" >
    <!ENTITY as "http://www.inmindcomputing.com/application/application-schema.owl#" >
    <!ENTITY pcs "http://www.inmindcomputing.com/application/products/products-schema.owl#" >
    <!ENTITY pcsmrb "http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#" >
    <!ENTITY psa "http://www.inmindcomputing.com/application/products/products-schema-assembly.owl#" >
]>


<rdf:RDF xmlns="http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#"
     xml:base="http://www.inmindcomputing.com/application/products/products-schema-mrb.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:pcs="http://www.inmindcomputing.com/application/products/products-schema.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:as="http://www.inmindcomputing.com/application/application-schema.owl#"
     xmlns:psa="http://www.inmindcomputing.com/application/products/products-schema-assembly.owl#"
     xmlns:pcsmrb="http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#"
     xmlns:ps="http://www.inmindcomputing.com/platform/platform-schema.owl#">
    <owl:Ontology rdf:about="http://www.inmindcomputing.com/application/products/products-schema-mrb.owl">
        <owl:imports rdf:resource="http://www.inmindcomputing.com/application/application-schema.owl"/>
        <owl:imports rdf:resource="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl"/>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyGVA -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyGVA">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">GVA inculsive of markup</rdfs:label>
        <rdfs:label xml:lang="zh">增加值总额包括加价</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyGVA)
          WHERE {

            ?subject pcs:assemblySubTotalE ?st .
            ?subject psa:assemblyPackaging ?ap .
      ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?st - ?ap) AS ?val)
        }</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:group rdf:resource="&pcsmrb;MRBAssemblyProfitPackagingDelivery"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyL6GVA -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyL6GVA">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">GVA inculsive of markup</rdfs:label>
        <rdfs:label xml:lang="zh">增加值总额包括加价</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyL6GVA)
          WHERE {

            ?subject pcs:assemblySubTotalF ?stf .
      ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?stf) AS ?val)
        }</ps:sparql>
        <ps:sequenceID rdf:datatype="&xsd;int">2</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <ps:group rdf:resource="&pcsmrb;MRBL6Assembly"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyPurchaseItemOthersGVA -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyPurchaseItemOthersGVA">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">GVA inclusive of markup</rdfs:label>
        <rdfs:label xml:lang="zh">增加值总额包括加价</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyPurchaseItemOthersGVA)
          WHERE {

            ?subject pcs:assemblyTotalPurchaseItemOthersSellingPrice ?sp .
            ?subject pcs:assemblyTotalPurchaseItemOthersMaterialCost ?mc .
      ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?sp - ?mc) AS ?val)
        }</ps:sparql>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <ps:group rdf:resource="&pcsmrb;MRBPurchasedPartOthers"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyPurchaseItemPlasticGVA -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyPurchaseItemPlasticGVA">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">GVA inclusive of markup</rdfs:label>
        <rdfs:label xml:lang="zh">增加值总额包括加价</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyPurchaseItemPlasticGVA)
          WHERE {

            ?subject pcs:assemblyTotalPurchaseItemPlasticSellingPrice ?sp .
            ?subject pcs:assemblyTotalPurchaseItemPlasticsMaterialCost ?mc .
       ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?sp - ?mc) AS ?val)
        }</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <ps:group rdf:resource="&pcsmrb;PlasticsPurchasedPart"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblySubTotalB1 -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblySubTotalB1">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">SUB-TOTAL B1 (Secondary Process) + Mark Up</rdfs:label>
        <rdfs:label xml:lang="zh">小计 B (二次加工工艺) + 加价</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (sum(?val) as ?assemblySubTotalB1)
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:includesSalesItem ?minorSi .
   ?minorSi as:includesConfigItem ?c .
   ?c pcsp:processFinalCostPerSet ?cps.
   ?salesItem as:isProduct pci:Assembly .
   ?minorSi as:isProduct pci:SecondaryProcess .
   BIND (xsd:decimal(?cps) AS ?val)
} GROUP BY ?subject
        </ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;MRBSubTotals"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblySubTotalB2 -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblySubTotalB2">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">SUB-TOTAL B2 (Secondary Finishing Process) + Mark Up</rdfs:label>
        <rdfs:label xml:lang="zh">小计 B (二次加工工艺) + 加价</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (sum(?val) as ?assemblySubTotalB2)
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:includesSalesItem ?minorSi .
   ?minorSi as:includesConfigItem ?c .
   ?c pcsp:processFinalCostPerSet ?cps.
   ?salesItem as:isProduct pci:Assembly .
   ?minorSi as:isProduct pci:SecondaryFinishingProcess .
   BIND (xsd:decimal(?cps) AS ?val)
} GROUP BY ?subject

        </ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">4</ps:sequenceID>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;MRBSubTotals"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyTotalAssemblyAndPackagingCost -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyTotalAssemblyAndPackagingCost">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Total Assembly &amp; Packaging Cost</rdfs:label>
        <rdfs:label xml:lang="zh">总装配和包装成本</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyTotalAssemblyAndPackagingCost)
          WHERE {

            ?subject pcs:assemblySubTotalE ?st .
      ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?st) AS ?val)
        }
</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">1</ps:sequenceID>
        <ps:group rdf:resource="&pcsmrb;MRBAssemblyProfitPackagingDelivery"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyTotalL6AssemblyCost -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyTotalL6AssemblyCost">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Total L6 Assembly Cost</rdfs:label>
        <rdfs:label xml:lang="zh">总级别6 的组装成本</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyTotalL6AssemblyCost)
          WHERE {

            ?subject pcs:assemblySubTotalF ?stf .
       ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?stf) AS ?val)
        }</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">1</ps:sequenceID>
        <ps:group rdf:resource="&pcsmrb;MRBL6Assembly"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyTotalPackagingCost -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyTotalPackagingCost">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Packaging Cost</rdfs:label>
        <rdfs:label xml:lang="zh">包装成本</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (?val as ?assemblyTotalPackagingCost)
WHERE {
   OPTIONAL{?subject psa:assemblyPackaging ?ap .}
   OPTIONAL{?subject psa:assemblyPkgCost ?pckSum. }
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:isProduct pci:Assembly .
   #BIND (xsd:decimal(?ap) AS ?val)
   BIND(IF(BOUND(?pckSum) &amp;&amp; ?pckSum != &quot;-&quot;,?pckSum, ?ap) as ?val)
}
</ps:sparql>
        <ps:readOnly rdf:datatype="&xsd;boolean">true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">2</ps:sequenceID>
        <ps:group rdf:resource="&pcsmrb;MRBAssemblyProfitPackagingDelivery"/>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#assemblyTotalRecoverPercentage -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;assemblyTotalRecoverPercentage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Percentage to Recover</rdfs:label>
        <rdfs:label xml:lang="zh">百分比恢复</rdfs:label>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">15</ps:sequenceID>
        <ps:sparql>SELECT ?subject (?val as ?assemblyTotalRecoverPercentage)
          WHERE {

            ?subject pcs:assemblyTotalRecoverAmount ?val1 .
            ?subject pcs:assemblyTotalRecoverQuoted ?val2 .
      ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
             BIND (xsd:decimal((?val2 / ?val1) * 100) AS ?val) .
        }</ps:sparql>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;SummaryTooling"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#costItemData -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;costItemData">
        <rdfs:subPropertyOf rdf:resource="&ps;businessData"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema-mrb.owl#mrbNonMFGSource -->

    <owl:DatatypeProperty rdf:about="&pcsmrb;mrbNonMFGSource">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Vendor</rdfs:label>
        <rdfs:label xml:lang="zh">供应商</rdfs:label>
        <as:costingRelevantSequence>21</as:costingRelevantSequence>
        <as:costingRelevant>true</as:costingRelevant>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <rdfs:subPropertyOf rdf:resource="&pcsmrb;costItemData"/>
        <ps:group rdf:resource="&pcs;PurchasePart"/>
        <ps:group rdf:resource="&pcs;Sales"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.inmindcomputing.com/application/products/products-schema-knowledgebase.owl#PurchasedPartSubMaterial-KB"/>
                    <rdf:Description rdf:about="&pcs;PurchasedPartOther-KB"/>
                    <rdf:Description rdf:about="&pcs;PurchasedPlasticPart-KB"/>
                    <rdf:Description rdf:about="&pcs;Subcon-KB"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#assemblyAccessoriesToolingCost -->

    <owl:DatatypeProperty rdf:about="&pcs;assemblyAccessoriesToolingCost">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Accessories - Tool Cost</rdfs:label>
        <rdfs:label xml:lang="zh">配件 -  模具成本</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (sum(?val) as ?assemblyAccessoriesToolingCost)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .

            ?salesItem as:includesSalesItem ?minorSi .

            ?minorSi as:includesConfigItem ?c .
            ?c pcs:mrbNonMFGToolingCost ?tc.
      ?salesItem as:isProduct pci:Assembly .
      ?minorSi as:isProduct pci:PurchasedPartOther .
            BIND (xsd:decimal(?tc) AS ?val)
          } GROUP BY ?subject
        </ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">10</ps:sequenceID>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;SummaryTooling"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#assemblyAccessoriesToolingRecoverAmount -->

    <owl:DatatypeProperty rdf:about="&pcs;assemblyAccessoriesToolingRecoverAmount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Accessories - Recover Amount</rdfs:label>
        <rdfs:label xml:lang="zh">配件 -  恢复数目</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (sum(?val) as ?assemblyAccessoriesToolingRecoverAmount)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .

            ?salesItem as:includesSalesItem ?minorSi .

            ?minorSi as:includesConfigItem ?c .
            ?c pcs:mrbRecoverAmount ?tc.
      ?minorSi as:isProduct pci:PurchasedPartOther .
       ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?tc) AS ?val)
          } GROUP BY ?subject
        </ps:sparql>
        <ps:sequenceID rdf:datatype="&xsd;int">11</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;SummaryTooling"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#assemblyAccessoriesToolingToolRecoverQuoted -->

    <owl:DatatypeProperty rdf:about="&pcs;assemblyAccessoriesToolingToolRecoverQuoted">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Accessories - Recover / Quoted</rdfs:label>
        <rdfs:label xml:lang="zh">配件 -  恢复/报价</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (sum(?val) as ?assemblyAccessoriesToolingToolRecoverQuoted)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .

            ?salesItem as:includesSalesItem ?minorSi .

            ?minorSi as:includesConfigItem ?c .
            ?c pcs:mrbRecoverQuoted ?tc.
      ?salesItem as:isProduct pci:Assembly .
       ?minorSi as:isProduct pci:PurchasedPartOther .
            BIND (xsd:decimal(?tc) AS ?val)
          } GROUP BY ?subject
        </ps:sparql>
        <ps:sequenceID rdf:datatype="&xsd;int">12</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;SummaryTooling"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#assemblyFinalPackageCostForAssy -->

    <owl:DatatypeProperty rdf:about="&pcs;assemblyFinalPackageCostForAssy">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">FINAL PACKAGE COST FOR ASSY (Ex-Factory)</rdfs:label>
        <rdfs:label xml:lang="zh">组装包装成本 （不含模具成本）</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (?val as ?assemblyFinalPackageCostForAssy)
          WHERE {
            ?subject as:configItemIncludedBy ?salesItem .

            ?subject pcs:assemblySubTotalA ?sta .
            ?subject pcs:assemblySubTotalB ?stb .
      ?subject pcsmrb:assemblySubTotalB1 ?stb1 .
      ?subject pcsmrb:assemblySubTotalB2 ?stb2 .
            ?subject pcs:assemblySubTotalC ?stc .
            ?subject pcs:assemblySubTotalD ?std .
            ?subject pcs:assemblySubTotalE ?ste .
            ?subject pcs:assemblySubTotalF ?stf .
      ?subject as:configItemIncludedBy ?salesItem .
            ?salesItem as:isProduct pci:Assembly .
            BIND (xsd:decimal(?sta + ?stb + ?stb1 + ?stb2 + ?stc + ?std + ?ste + ?stf) AS ?val)
        }</ps:sparql>
        <ps:sequenceID rdf:datatype="&xsd;int">11</ps:sequenceID>
        <ps:readOnly>true</ps:readOnly>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;MRBSubTotals"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#assemblyFinishingCostFromVendor -->

    <owl:DatatypeProperty rdf:about="&pcs;assemblyFinishingCostFromVendor">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Finishing Cost from Vendor</rdfs:label>
        <rdfs:label xml:lang="zh">从供应商的总表面或加工处理成本</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject (sum(?val) as ?assemblyFinishingCostFromVendor)
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:includesSalesItem ?minorSi .
   ?minorSi as:includesConfigItem ?c .
   ?c pcs:mrbNonMFGActCostPerSet ?cps.
   ?salesItem as:isProduct pci:Assembly .
   ?minorSi as:isProduct pci:Subcon .
   BIND (xsd:decimal(?cps) AS ?val)
} GROUP BY ?subject</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID rdf:datatype="&xsd;int">2</ps:sequenceID>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;SubconMRBCost"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
