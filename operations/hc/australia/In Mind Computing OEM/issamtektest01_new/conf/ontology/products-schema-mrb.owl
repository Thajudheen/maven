    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#mrbNonMFGactlCostPerPiece -->

    <owl:DatatypeProperty rdf:about="&pcs;mrbNonMFGactlCostPerPiece">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>Act. Cost / Pc</rdfs:label>
        <rdfs:label xml:lang="zh">每件实际成本</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT  ?subject (?val AS ?mrbNonMFGactlCostPerPiece)
WHERE {
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:salesItemIncludedBy+ ?quote .
   ?quote rdf:type as:Quote .
   ?quote ase:zQuoteActiveMOQ ?activemoq.
   #?salesItem as:isProduct pci:PurchasedPartSubMaterial.

   OPTIONAL{
      ?subject pcs:purchasedPartPowderCoatingCostPerPc ?powdercoating.
   }
   OPTIONAL{
      ?subject pcs:purchasedPartMaskingCostPerPc ?masking.
   }
  OPTIONAL{
      ?subject pcs:actCostPerPieceMOQ1 ?acppmoq1.
   }
   OPTIONAL{
      ?subject pcs:actCostPerPieceMOQ2 ?acppmoq2.
   }
   OPTIONAL{
      ?subject pcs:actCostPerPieceMOQ3 ?acppmoq3.
   }
   OPTIONAL{
      ?subject pcs:actCostPerPieceMOQ4 ?acppmoq4.
   }
   OPTIONAL{
      ?subject pcs:actCostPerPieceMOQ5 ?acppmoq5.
   }
   BIND (IF(BOUND(?powdercoating) &amp;&amp; ?powdercoating != &quot;-&quot;,
         ?powdercoating,
         IF(BOUND(?masking) &amp;&amp; ?masking != &quot;-&quot;,
            ?masking,
            IF( str(?activemoq )= &quot;MOQ1&quot;,
               ?acppmoq1,
               IF ( str(?activemoq) = &quot;MOQ2&quot;,
                  ?acppmoq2  ,
                  IF (str( ?activemoq) = &quot;MOQ3&quot;,
                     ?acppmoq3  ,
                     IF ( str(?activemoq) = &quot;MOQ4&quot;,
                        ?acppmoq4  ,
                        IF ( str(?activemoq) = &quot;MOQ5&quot;,
                           ?acppmoq5 ,
                           xsd:decimal(0)))))))) AS ?val)
}

</ps:sparql>
