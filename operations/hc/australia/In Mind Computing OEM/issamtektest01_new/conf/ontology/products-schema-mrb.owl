    <!-- http://www.inmindcomputing.com/application/products/products-schema.owl#assemblySubTotalE -->

    <owl:DatatypeProperty rdf:about="&pcs;assemblySubTotalE">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>SUB-TOTAL : E (ASSY / PROFIT / PKG+DEL)</rdfs:label>
        <rdfs:label xml:lang="zh">小计 E (组装/利润/包装+交货)</rdfs:label>
        <ps:defaultValue rdf:datatype="&xsd;decimal">0</ps:defaultValue>
        <ps:sparql rdf:datatype="&xsd;string">select ?subject (?val as ?assemblySubTotalE)
WHERE {

   ?subject psa:assemblyLabourAndStorageCost ?lsc .
   ?subject psa:assemblyGA ?ga .
   OPTIONAL
   {
      ?subject psa:assemblyPackaging ?pck .
      ?subject psa:assemblyPackagingMarkupAbsolute ?pma .
   }
   OPTIONAL{
      ?subject psa:assemblyPkgCost ?pckSum.
   }

   ?subject psa:assemblyProfit ?p.
   ?subject as:configItemIncludedBy ?salesItem .
   ?salesItem as:isProduct pci:Assembly .
   BIND(IF(BOUND(?pckSum) &amp;&amp; ?pckSum !=&quot;-&quot;,xsd:decimal(?lsc + ?ga + ?pckSum + ?p),xsd:decimal(?lsc + ?ga + ?pck + ?pma + ?p)) AS ?val )
          }
</ps:sparql>
        <ps:readOnly>true</ps:readOnly>
        <ps:sequenceID>9</ps:sequenceID>
        <rdfs:domain rdf:resource="&pcs;Assembly-KB"/>
        <ps:group rdf:resource="&pcs;MRBSubTotals"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>



