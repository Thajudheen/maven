        <ps:readOnly rdf:datatype="&xsd;boolean">true</ps:readOnly>
        <ps:defaultValue xml:lang="de">1080 Euro</ps:defaultValue>
        <ps:defaultValue xml:lang="en">1000USD, 1100 SGD</ps:defaultValue>
        <ps:group rdf:resource="&ai;Base"/>
