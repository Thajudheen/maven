SELECT ?subject ?itemHeaderPrice
WHERE { 
   ?subject ase:itemHeaderSubPrice1 ?subValue1 .
   ?subject ase:itemHeaderSubPrice ?subValue .
   BIND(xsd:decimal(?subValue + ?subValue1) AS ?itemHeaderPrice)
}                </ps:sparql>
