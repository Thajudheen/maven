        <rdfs:label rdf:datatype="&xsd;string">Sub Item Value</rdfs:label>
        <rdfs:domain rdf:resource="&as;ItemHeaderPriceItem"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
