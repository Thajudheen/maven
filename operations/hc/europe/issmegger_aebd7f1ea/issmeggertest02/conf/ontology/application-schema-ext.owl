    <!-- http://www.inmindcloud.com/application/application-schema-ext.owl#itemHeaderApprovalDiscount -->

    <owl:DatatypeProperty rdf:about="&ase;itemHeaderApprovalDiscount">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label rdf:datatype="&xsd;string">Approval Discount Calculation</rdfs:label>
        <ps:sparql rdf:datatype="&xsd;string">SELECT ?subject ?itemHeaderApprovalDiscount
  WHERE {
   ?subject  as:includesPriceItem ?item.
   ?item as:hasPriceItemType ?type.
   ?type as:objectERPId &quot;ZMDT&quot;.
   ?item as:priceItemBaseAmount ?itemHeaderApprovalDiscount.
}
	  </ps:sparql>
        <rdfs:domain rdf:resource="&as;ItemHeaderPriceItem"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


