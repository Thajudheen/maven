 <!-- http://www.inmindcloud.com/application/application-schema-ext.owl#zProdTime  -->

    <owl:DatatypeProperty rdf:about="&ase;zProdTime">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Delivery Time (days)</rdfs:label>
        <ps:readOnly rdf:datatype="&xsd;boolean">true</ps:readOnly>
        <ps:sequenceID>2</ps:sequenceID>
        <ps:sparql>SELECT ?subject ?zProdTime 
WHERE
{
   ?subject as:includesDocumentHeaderPriceItem ?docPi.
   ?docPi as:documentHeaderNetValue ?netVal.
   {
      SELECT (MAX(?o) as ?leadTime) WHERE {
         ?subject as:includesSalesItem+ ?si.
         ?si as:includesItemHeaderPriceItem ?ihpi.
         ?ihpi as:includesPriceItem ?priceItemDel.
         ?priceItemDel  as:hasPriceItemType ?ptype.
         ?ptype ps:objectName "Delivery Days".
        ?priceItemDel as:includesBaseAmount ?ba.
         ?ba as:amountValue ?o
                  
      }
   }
   BIND (
      IF (?netVal > xsd:decimal(3000000),
         xsd:decimal(7) + ?leadTime + xsd:decimal(21) + xsd:decimal(14), 
         xsd:decimal(xsd:decimal(7) + ?leadTime + xsd:decimal(21))) as ?zProdTime)
}


</ps:sparql>
        <ps:group rdf:resource="&ase;AdditionalAttributesQuoteMTO"/>
        <rdfs:domain rdf:resource="&as;Quote"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>

	 <!-- http://www.inmindcloud.com/application/application-schema-ext.owl#zPricingSleeving  -->

    <owl:DatatypeProperty rdf:about="&ase;zPricingSleeving">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Sleeving from SB to Coppoer for Pricing</rdfs:label>
        <ps:readOnly rdf:datatype="&xsd;boolean">true</ps:readOnly>
        <ps:sequenceID>2</ps:sequenceID>
        <ps:sparql>
		SELECT ?subject ?zPricingSleeving
          WHERE { 
?subject as:salesItemIncludedBy+ ?salesItem .
  ?salesItem as:isProduct ?prod.
   ?prod ps:objectName &quot;LV-SB-MF&quot;.
   ?salesItem as:includesConfigItem ?conf.
     BIND(ps:configItemDAValueFinder2(?conf, &quot;LV-SB-MF_BB_SLEEVE_U&quot; , &quot;def&quot; ) as ?t)
   BIND( uri(?t) as ?s)
   ?s ps:objectName ?o .
   BIND (IF( ?o = &quot;SB_BB_SLEEVING_NO&quot;, &quot;NO&quot; , &quot;YES&quot; ) as ?zPricingSleeving)			   
}
</ps:sparql>
        <rdfs:domain rdf:resource="&as;SalesItem"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
	
