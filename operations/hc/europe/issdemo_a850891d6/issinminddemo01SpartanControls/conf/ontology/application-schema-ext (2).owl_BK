BIND (xsd:decimal(?p)  AS ?val)
  }</ps:sparql>
        <ps:readOnly rdf:datatype="&xsd;boolean">true</ps:readOnly>
        <ps:group rdf:resource="&ase;GroupDimensionData"/>
        <rdfs:domain rdf:resource="&as;Quote"/>
