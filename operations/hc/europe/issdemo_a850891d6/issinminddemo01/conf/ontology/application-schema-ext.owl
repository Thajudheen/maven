    <!-- http://www.inmindcloud.com/application/application-schema-ext.owl#zProjectVolume -->

    <owl:DatatypeProperty rdf:about="&ase;zProjectVolume">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="de">Projekt Volumen</rdfs:label>
        <rdfs:label xml:lang="en">Project Volume</rdfs:label>
        <ps:sequenceID rdf:datatype="&xsd;int">3</ps:sequenceID>
        <rdfs:comment rdf:datatype="&xsd;string">SELECT ?subject (?val as ?zProjectVolume)
  WHERE {
   ?subject as:includesDocumentHeaderPriceItem ?dhp.
    ?dhp as:documentHeaderPrice ?p.
    ?subject ase:zProductLifeSpan ?pls .
?subject ase:zQuantityPerMonth ?qpm .
BIND (xsd:decimal( ?pls *?p * ?qpm)  AS ?val)
     }
  </rdfs:comment>
        <ps:readOnly rdf:datatype="&xsd;boolean">true</ps:readOnly>
        <ps:group rdf:resource="&ase;GroupQuoteAdditionalAttributes"/>
        <rdfs:domain rdf:resource="&as;Quote"/>
        <rdfs:subPropertyOf rdf:resource="&ps;calculation"/>
        <rdfs:subPropertyOf rdf:resource="&ps;materialization"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


