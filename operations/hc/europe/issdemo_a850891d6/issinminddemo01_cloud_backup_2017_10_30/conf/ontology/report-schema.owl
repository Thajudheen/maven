CONSTRUCT {
   ?uuid a ase:ProspectsPerRegionData;
      ps:orderBy ?vol;
      a ps:Data;
      ase:amount ?amount;
      ase:regionaxis1 ?reg.
} WHERE {
   {


SELECT (?labelregion AS ?reg )    (COUNT(?person) as ?amount) WHERE {
{         ?prospect as:hasAccountStatus as:ACTIVE.
         ?prospect rdf:type as:Account.
         ?prospect as:accountProspect ?accProsp.
   ?prospect ps:objectName ?accountname.
         ?prospect as:includesPerson ?person.
        ?address as:addressIncludedBy ?prospect.
         
         ?prospect as:usesMainAddress ?address.
         ?address as:hasCountry ?country.
         ?country as:hasRegion ?region.
         ?region ps:objectName ?labelregion.
         ?person as:personEmail ?email.
         FILTER (xsd:string(?accProsp) = &quot;true&quot;).
         FILTER(xsd:string(?mainAd) = &quot;true&quot;).

       ?prospect as:containsOpportunity ?opp.
        ?opp as:opportunityVolumeSystemCurrency ?vol.
?opp as:hasSalesDocumentStatus as:OPEN.
      FILTER (?vol = 0)
}   UNION
      {
         ?prospect as:hasAccountStatus as:ACTIVE.
         ?prospect rdf:type as:Account.
         ?prospect as:accountProspect ?accProsp.
   ?prospect ps:objectName ?accountname.
         ?prospect as:includesPerson ?person.
         ?prospect as:usesMainAddress ?address.
         ?address as:hasCountry ?country.
         ?country as:hasRegion ?region.
         ?region ps:objectName ?labelregion.
         ?person as:personEmail ?email.
         FILTER (xsd:string(?accProsp) = &quot;true&quot;).
         FILTER(xsd:string(?mainAd) = &quot;true&quot;).
      OPTIONAL {?prospect as:containsOpportunity ?opp.}
    FILTER(!BOUND(?opp))
   }
UNION
{         ?prospect as:hasAccountStatus as:ACTIVE.
         ?prospect rdf:type as:Account.
         ?prospect as:accountProspect ?accProsp.
   ?prospect ps:objectName ?accountname.
         ?prospect as:includesPerson ?person.
         ?prospect as:usesMainAddress ?address.
         ?address as:hasCountry ?country.
         ?country as:hasRegion ?region.
         ?region ps:objectName ?labelregion.
         ?person as:personEmail ?email.
         FILTER (xsd:string(?accProsp) = &quot;true&quot;).
         FILTER(xsd:string(?mainAd) = &quot;true&quot;).

       ?prospect as:containsOpportunity ?opp.

?opp as:hasSalesDocumentStatus as:OPEN.
      OPTIONAL {?opp as:opportunityVolumeSystemCurrency ?vol.}
    FILTER(!BOUND(?vol))
}
   }  GROUP BY ?labelregion


  }


BIND(afn:uuid() as ?uuid).
}</ps:sparql>
