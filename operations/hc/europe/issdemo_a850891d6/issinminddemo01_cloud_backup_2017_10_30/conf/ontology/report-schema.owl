 FILTER (xsd:string(?admain) = &apos;true&apos;).
 FILTER (xsd:string(?accnl) = &apos;false&apos;).
  FILTER (xsd:string(?acpros) = &apos;true&apos;).
}GROUP BY ?labelregion ?accountName ?cFirstname ?cLastname ?cEmail ?cPosition ?contactTitleStr

  }
  BIND(afn:uuid() as ?uuid).
}
