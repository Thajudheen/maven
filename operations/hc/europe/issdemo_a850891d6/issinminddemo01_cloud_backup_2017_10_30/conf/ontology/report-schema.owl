            ?opp  as:hasSalesDocumentStatus as:OPEN.
            ?opp ps:objectDateOfCreation ?oppdate .
            FILTER(xsd:string(?mainAd) = &quot;true&quot;).
            FILTER(xsd:string(?tagname) = &quot;Outbound Lead&quot;).
            BIND(xsd:dateTime(xsd:dateTime(now()) + &quot;-P30D&quot;^^xsd:duration) AS ?date30before).
            BIND(xsd:dateTime(xsd:dateTime(now()) + &quot;-P60D&quot;^^xsd:duration) AS ?date60before).
            Filter(?oppdate &gt; ?date60before).
            Filter(?oppdate &lt; ?date30before).
            FILTER (xsd:string(?accProsp) = &quot;true&quot;)
         }
         BIND(&apos;L30-60&apos; AS ?type).
      } GROUP BY ?labelregion ?type
   }
   BIND(afn:uuid() as ?uuid).
}





</ps:sparql>
