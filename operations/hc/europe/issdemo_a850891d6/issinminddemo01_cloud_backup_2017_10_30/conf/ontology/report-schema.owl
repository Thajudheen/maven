        <ps:hasMeasure rdf:resource="&ase;MeasureActivity"/>
        <ps:hasMeasure rdf:resource="&ase;MeasureMood"/>
        <ps:hasMeasure rdf:resource="&ase;MeasureWeightedVolumeGroup3"/>
