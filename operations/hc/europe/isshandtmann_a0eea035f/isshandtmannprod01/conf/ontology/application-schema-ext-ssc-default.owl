    <!-- http://www.inmindcomputing.com/ssc/submodel.owl#ApplicationField-KUNNR -->

    <rdf:Description rdf:about="&ssc;ApplicationField-KUNNR">
        <ssc:valueQuery rdf:datatype="&xsd;string">
			SELECT DISTINCT ?val WHERE { 
				?accnt rdf:type as:Account . 
				?quote as:quoteContainedBy ?accnt . 
				?accnt as:accountSAPID ?val . 
			}</ssc:valueQuery>
