//This is for the visualization and popping up of different cstics
rule INIT_HLP_VIS {
  objects: 
    ?O is_a (300)MOBILEEXCAVATOR
  body:
    then increment: ?O.HLP_VIS by 0
}

rule SET_HLP_VIS {
  objects: 
    ?O is_a (300)MOBILEEXCAVATOR
  condition:
 	?O.PAINTING specified
  body:
    then increment: ?O.HLP_VIS by 1
	
}
