ruleNet MOBILEEXCAVATOR_RNET {
	
	rules
	LISTPRICE_2_PRODUCTPRICE,
	CABIN_PRODUCTPRICE,
	DIPPER_PRODUCTPRICE,
	PAINTING_PRODUCTPRICE,
	PAINTING_PRODUCTPRICE2,
	OIL_PRODUCTPRICE,
	ATTACHEMENTS_PRODUCTPRICE,
	SEAT_PRODUCTPRICE,
	ATTACHEMENTS2_PRODUCTPRICE,
	  INIT_HLP_VIS,
	  LISTPRICE_3_PRODUCTPRICE,
	  MIRROR_PRODUCTPRICE,
    SET_HLP_VIS
	}
	
	rule LISTPRICE_2_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
	body:
		then increment: ?O.PRICE_AGGR by ?O.PRODUCTLISTPRICE		
}
rule LISTPRICE_3_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)CABIN
	body:
		then increment: ?O.PRICE_AGGR by ?O.PRODUCTLISTPRICE		
}
rule CABIN_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.EXCAVATOR_TYPE = 'CAB'
	body:
		then increment: ?O.PRICE_AGGR by 7300
}
rule MIRROR_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)CABIN
    condition:
    	?O.MIRROR = 'EXTMIRROR'
	body:
		then increment: ?O.PRICE_AGGR by 460
}
rule DIPPER_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.DIPPER = 'LONG'
	body:
		then increment: ?O.PRICE_AGGR by 1800
}
rule SEAT_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)CABIN
    condition:
    	?O.SEAT = 'AIRCUSHIONED'
	body:
		then increment: ?O.PRICE_AGGR by 350
}
rule PAINTING_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.PAINTING = 'BODY_CUSTOM'
	body:
		then increment: ?O.PRICE_AGGR by 550
}
rule PAINTING_PRODUCTPRICE2 {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.PAINTING = 'CABIN_CUSTOM'
	body:
		then increment: ?O.PRICE_AGGR by 650
}
rule OIL_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.OIL = 'BIO'
	body:
		then increment: ?O.PRICE_AGGR by 450
}
rule ATTACHEMENTS_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.ATTACHMENTS = 'SHH'
	body:
		then increment: ?O.PRICE_AGGR by 1300
}
rule ATTACHEMENTS2_PRODUCTPRICE {
  	objects: 
    	?O is_a (300)MOBILEEXCAVATOR
    condition:
    	?O.ATTACHMENTS = 'BHH'
	body:
		then increment: ?O.PRICE_AGGR by 2500
}
