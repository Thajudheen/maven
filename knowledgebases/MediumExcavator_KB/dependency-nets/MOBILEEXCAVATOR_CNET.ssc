constraint MAINTENANCE_PRICE_CS {
	objects
		?C is_a (300) MAINTENANCE
	restrictions
		table MAINTENANCE_PRICE (
			CONTRACT_SERVICE_TERM = ?C.CONTRACT_SERVICE_TERM,
			CONTRACT_SUPPORT_TYPE = ?C.CONTRACT_SUPPORT_TYPE,
			PRODUCTPRICE = ?C.PRODUCTPRICE
		)
	inferences
		?C.PRODUCTPRICE
}

