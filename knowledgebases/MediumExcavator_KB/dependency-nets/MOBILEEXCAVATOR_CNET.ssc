constraintNet ENTERPRISE_ROUTER_CNET {
  constraints
   BILD_CS,
    CREATE_CONTRACT
    ,CREATE_CABIN
    ,CREATE_CANOPY
    ,CABIN_PAINTING
    ,PAINTING_INVIS
    ,AGGR_2_PP
    ,MAINTENANCE_PRICE_CS
    ,AGGR_3_PP
}

constraint CABIN_PAINTING{
objects ?O is_a(300)MOBILEEXCAVATOR
		
		restrictions
		
		table CABIN_PAINTING (
			EXCAVATOR_TYPE = ?O.domain EXCAVATOR_TYPE
			,PAINTING = ?O.domain PAINTING	
)

		inferences:
			?O.domain EXCAVATOR_TYPE, ?O.domain PAINTING
}

constraint PAINTING_INVIS {
	objects ?O is_a(300)MOBILEEXCAVATOR
	condition:
		?O.PAINTING ne 'BODY_CUSTOM'  or  ?O.HLP_VIS = 0
	
	restrictions:
		
		?O.invisible CUSTOMPAINTING
			
		
	inferences:
		?O.invisible CUSTOMPAINTING
}
constraint BILD_CS {
	objects ?O is_a(300)MOBILEEXCAVATOR
	condition:
		?O.EXCAVATOR_TYPE specified
	
	restrictions:
		
		?O.PRODUCTIMAGE = 'CABIN.jpg'
			if ?O.EXCAVATOR_TYPE = 'CAB'
		,?O.PRODUCTIMAGE = 'CANOPY.jpg'
			if ?O.EXCAVATOR_TYPE = 'CANOPY'
		
	inferences:
		?O.PRODUCTIMAGE
}




constraint AGGR_2_PP {
	objects ?O is_a(300)MOBILEEXCAVATOR
	
	restrictions:
		?O.PRODUCTPRICE = ?O.PRICE_AGGR
	
	 inferences
	 ?O.PRODUCTPRICE
}
constraint AGGR_3_PP {
	objects ?O is_a(300)CABIN
	
	restrictions:
		?O.PRODUCTPRICE = ?O.PRICE_AGGR
	
	 inferences
	 ?O.PRODUCTPRICE
}

constraint CREATE_CONTRACT {
	objects
		?O is_object(material) (300) (nr=MOBILEEXCAVATOR)
	
	condition:
		?O.EXCAVATOR_TYPE specified
	
	restrictions:
		has_part(
			?O, 
			pos_type=(material)(300)(nr=MAINTENANCE)

		) 
}
constraint CREATE_CABIN {

	objects
		?O is_object(material) (300) (nr=MOBILEEXCAVATOR)
	
	condition:
		?O.EXCAVATOR_TYPE specified
restrictions:
		has_part(
			?O, 
			pos_type=(material)(300)(nr=CABIN)
		) if ?O.EXCAVATOR_TYPE = 'CAB'
}
constraint CREATE_CANOPY {

	objects
		?O is_object(material) (300) (nr=MOBILEEXCAVATOR)
	
	condition:
		?O.EXCAVATOR_TYPE specified
restrictions:
		has_part(
			?O, 
			pos_type=(material)(300)(nr=CANOPY)
		) if ?O.EXCAVATOR_TYPE = 'CANOPY'

}

